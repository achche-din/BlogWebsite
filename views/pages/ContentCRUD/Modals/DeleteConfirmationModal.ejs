<!-- Delete Confirmation Modal -->
<div class="modal fade" id="DeleteConfirmationModalCenter<%=ind%>" tabindex="-1" role="dialog" aria-labelledby="ConfirmationModalCenterTitle<%=ind%>" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered" role="document">
      <div class="modal-content">
        <div class="modal-header">
          <h5 class="modal-title" id="DeleteConfirmationModalLongTitle<%=ind%>">Are you sure you want to Delete?</h5>

          <button type="button" class="close" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">&times;</span>
          </button>
        </div>
        <div class="modal-body">
          <form method="GET" action="/blog/<%=subject%>/<%=topic%>/<%=subTopic%>/delete">
            <div class="form-group">
              <h5 type="text"><b>Topic:</b> <%=topic%></h5>
              <h5 type="text"><b>Title:</b> <%=subTopic%></h5>
            </div>
              <div class="modal-footer">
                  <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
                  <button type="submit" class="btn btn-danger">Delete</button>
              </div>
          </form>
        </div>
      </div>
    </div>
  </div>